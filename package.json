    "@angular/router": "^5.2.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "fscreen": "^1.0.2",
    "ngx-gauge": "1.0.0-beta.4",
    "nipplejs": "0.6.8",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
