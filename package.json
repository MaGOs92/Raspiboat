  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "build:webapp": "ng build --prod",
    "build:server": "cd server && npm i && npm run compile",
    "build:clean": "rm -r build/ && mkdir build",
    "build:moveDeps": "mv dist build && cp -r server build",
